@model IEnumerable<LoadDist.Models.DataModels.Subject>

<p>
    @Html.ActionLink("Добавить предмет", "AddSubject", "Lecturers", new { id = ViewData["id"]}, null)
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
    </tr>
}

</table>
